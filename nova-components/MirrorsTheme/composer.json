{
    "name": "klabs/mirrors-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Klabs\\MirrorsTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klabs\\MirrorsTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
